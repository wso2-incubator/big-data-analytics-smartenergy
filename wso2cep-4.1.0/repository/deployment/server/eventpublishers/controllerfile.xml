<?xml version="1.0" encoding="UTF-8"?>
<eventPublisher name="controllerfile" statistics="disable"
  trace="disable" xmlns="http://wso2.org/carbon/eventpublisher">
  <from streamName="controller" version="1.0.0"/>
  <mapping customMapping="enable" type="text">
    <inline>{{air_temperature}},
    </inline>
  </mapping>
  <to eventAdapterType="file">
    <property name="buffersize">1024</property>
    <property name="filename">/home/sanjaya/Desktop/controllerfile.csv</property>
  </to>
</eventPublisher>
